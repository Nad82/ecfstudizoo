generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@internal/prisma-second/client"
}

datasource db {
  provider = "mongodb"
  url      = env("SECOND_DATABASE_URL")
}

model Stat_animal {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  clic        Int
  date_created DateTime @db.Date
  prenom      String
}

model User {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  email       String
  password    String
}
